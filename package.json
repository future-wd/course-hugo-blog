{
  "name": "course-hugo-scss",
  "version": "0.0.1",
  "description": "Skillshare course - build scss with hugo",
  "main": "index.js",
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc",
    "dev:preview": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc --buildDrafts --buildFuture",
    "build": "exec-bin node_modules/.bin/hugo/hugo -v --gc --minify --cleanDestinationDir",
    "renderToDisk": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --renderToDisk --cleanDestinationDir",
    "renderToDisk:preview": "exec-bin node_modules/.bin/hugo/hugo server --renderToDisk --cleanDestinationDir --disableFastRender --buildDrafts --buildFuture ",
    "renderToDisk:production": "exec-bin node_modules/.bin/hugo/hugo server --renderToDisk --cleanDestinationDir --disableFastRender --gc --minify --environment production",
    "npm-check": "npx npm-check -u",
    "hugo-version": "exec-bin node_modules/.bin/hugo/hugo version",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/future-wd/course-hugo-bootstrap-scss.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/future-wd/course-hugo-bootstrap-scss/issues"
  },
  "homepage": "https://github.com/future-wd/course-hugo-bootstrap-scss#readme",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "exec-bin": "^1.0.0",
    "hugo-installer": "^3.1.0",
    "npm-check": "^5.9.2",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2"
  },
  "otherDependencies": {
    "hugo": "0.89.4"
  }
}
